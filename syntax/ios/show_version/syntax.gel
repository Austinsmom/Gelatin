define ws   /\s+/
define nows /\S+/
define fs   /[\t ]+/
define nl   /[\r\n]+/
define nonl /[^\r\n]+/

grammar input:
    match ws:
        do.next()
    match /IOS [^\r\n]+ Version / /[^,]+/ nonl nl:
        out.add('version', '$1')
    match 'cisco ' /\d+\S+/ nonl nl:
        out.add('model', '$1')
    match nonl nl:
        do.skip()
